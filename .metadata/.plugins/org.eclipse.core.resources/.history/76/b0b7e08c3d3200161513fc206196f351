package com.webcom.page;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.testng.Assert;

/**
 * @author guilherme.cutovoi
 *
 *
 * Esta pagina contem a lista dos clientes cadastrados 
 *
 */
public class ListaCadastroClientePage {

WebDriver driver;
	
	By buttonAdicionaCliente=By.className("btn btn-default pull-right");
	
	public ListaCadastroClientePage(WebDriver driver)
	{
		this.driver=driver;
	}
	
	public void ClicaButtonAdicionarCliente()
	{
		driver.findElement(buttonAdicionaCliente).click();
	}
	
	
}
